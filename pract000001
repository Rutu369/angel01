#include <iostream>
using namespace std;

int main() {
    string name;
    int age;
    bool eligible;

    // Input
    cout << "Enter your name: ";
    cin >> name;

    cout << "Enter your age: ";
    cin >> age;

    // Check eligibility
    if (age >= 18)
        eligible = true;
    else
        eligible = false;

    // Output
    cout << "\n--- Voting Eligibility Result ---\n";
    cout << "Name: " << name << endl;
    cout << "Age: " << age << endl;

    if (eligible)
        cout << "Status: Eligible to Vote" << endl;
    else
        cout << "Status: Not Eligible to Vote" << endl;

    return 0;
}